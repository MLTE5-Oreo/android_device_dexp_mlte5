<manifest version="1.0" type="device">
    <!-- audio -->
    <hal format="hidl">
        <name>android.hardware.audio</name>
        <transport arch="32">passthrough</transport>
        <impl level="generic"></impl>
        <version>2.0</version>
    </hal>
    <hal format="hidl">
        <name>android.hardware.audio.effect</name>
        <transport arch="32">passthrough</transport>
        <impl level="generic"></impl>
        <version>2.0</version>
    </hal>
    <hal format="hidl">
        <name>android.hardware.soundtrigger</name>
        <transport arch="32">passthrough</transport>
        <impl level="generic"></impl>
        <version>2.0</version>
        <interface>
            <name>ISoundTriggerHw</name>
            <instance>sound_trigger.primary</instance>
        </interface>
    </hal>
    <!-- /audio -->
    <!-- display -->
    <hal format="hidl">
        <name>android.hardware.graphics.allocator</name>
        <transport arch="32">passthrough</transport>
        <impl level="generic"></impl>
        <version>2.0</version>
        <interface>
            <name>IAllocator</name>
            <instance>default</instance>
        </interface>
    </hal>
    <!-- /display -->
    <!-- gnss -->
    <hal format="hidl">
        <name>android.hardware.gnss</name>
        <transport arch="32">passthrough</transport>
        <version>1.0</version>
        <interface>
            <name>IGnss</name>
            <instance>default</instance>
        </interface>
    </hal>
    <!-- /gnss -->
    <hal format="hidl">
        <name>android.hardware.wifi</name>
        <transport>hwbinder</transport>
        <version>1.0</version>
        <interface>
            <name>IWifi</name>
            <instance>default</instance>
        </interface>
    </hal>
    <!-- WIFI -->
    <hal format="hidl">
        <name>android.hardware.wifi.supplicant</name>
        <transport>hwbinder</transport>
        <version>1.0</version>
        <interface>
            <name>ISupplicant</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="hidl">
        <name>android.hardware.camera</name>
        <transport arch="32">passthrough</transport>
        <version>1.0</version>
        <interface>
            <name>ICameraProvider</name>
            <instance>legacy/1</instance>
        </interface>
    </hal>
    <!-- /WIFI -->
    <!-- video omx hal -->
    <hal format="hidl">
        <name>android.hardware.media</name>
        <transport arch="32">passthrough</transport>
        <impl level="generic"></impl>
        <version>1.0</version>
    </hal>
    <hal format="hidl">
        <name>android.hardware.media.omx</name>
        <transport arch="32">passthrough</transport>
        <impl level="generic"></impl>
        <version>1.0</version>
    </hal>
    <!-- /video omx hal -->
    <!-- sensors -->
    <hal format="hidl">
        <name>android.hardware.sensors</name>
        <transport arch="32">passthrough</transport>
        <version>1.0</version>
        <interface>
            <name>ISensors</name>
            <instance>default</instance>
        </interface>
    </hal>
    <!-- /sensors -->
</manifest>
